import java.awt.*;
import javax.swing.*;

public class Window extends JPanel{
    //the pictures
    ImageIcon map = Stuff.ImageResizer(new ImageIcon("images/crtestmap.png"), 700, 1100);
    JLabel guy = new JLabel(map);
    JPanel panel = new JPanel();

    Window(){
        setSize(700,1080);
        //setMinimumSize(new Dimension(700, 600));
        //setBackground(Color.gray);

        guy.setAlignmentX(40);
        guy.setAlignmentY(80);

        add(panel);
        panel.add(guy);

        setVisible(true);
    }
}
